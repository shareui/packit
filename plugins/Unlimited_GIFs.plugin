__id__ = "Unlimited_GIFs"
__name__ = "Unlimited GIFs"
__description__ = "Removes the limit on the number of Saved GIFs via Xposed hooks"
__version__ = "1.1.0"
__author__ = "@ArThirtyFour | @KangelPlugins"
